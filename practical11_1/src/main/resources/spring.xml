<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

 <bean id="mark" class="practical11_1.Mark">
	 <property name="maths" value="50"></property>
	 <property name="physics" value="48"></property>
	 <property name="chemistry" value="45"></property>
 </bean>
 <bean id="st" class="practical11_1.Student">
	 <property name="stId" value="30061"></property>
	 <property name="stName" value="Pradeep"></property>
	 <property name="mark" ref="mark"></property>
 </bean>


</beans>
